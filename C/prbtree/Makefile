include $(dir $(lastword $(MAKEFILE_LIST)))../../top.mk
include $(MTOP)/defs.mk

TO_MAKE := lib.mk

ifdef OS_WINXX
# $(PRODUCT_NAMES_H) - header generated by next included ../version.mk
# $(PRODUCT_NAMES_H) - also used in $(MTOP)/WINXX/cres.mk
# $(PRODUCT_NAME)    - used in $(MTOP)/WINXX/cres.mk
PRODUCT_NAMES_H := prbtree_vers.h
PRODUCT_NAME    := $(PRBTREE_DESC)
TO_MAKE += ../version.mk
endif

ifneq ($(filter check tests clean,$(MAKECMDGOALS)),)
TO_MAKE += test.mk
endif

include $(MTOP)/parallel.mk
